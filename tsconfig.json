{
	"compilerOptions": {
		"target": "ES2022",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"outDir": "./dist",
		"rootDir": "."
	},
	"exclude": ["node_modules"],
	"include": ["./**/*.ts"]
}
